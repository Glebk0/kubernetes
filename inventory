local ansible_connection=local
kuber-master ansible_host=192.168.1.80 ansible_ssh_private_key_file=.vagrant/machines/kubernetes-master/virtualbox/private_key
kuber-node ansible_host=192.168.1.81 ansible_ssh_private_key_file=.vagrant/machines/kubernetes-node/virtualbox/private_key


[all:vars]
ansible_connection=ssh
ansible_user=vagrant
